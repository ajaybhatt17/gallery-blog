    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.3.2",
    "redux-thunk": "^2.1.0",
    "express": "^4.16.2",
    "db-cdn": "^0.0.1"
